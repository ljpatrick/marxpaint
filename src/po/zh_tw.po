msgstr "工具"

msgid "Colors"
msgstr "顏色"

msgid "Brushes"
msgstr "筆刷"

msgid "Erasers"
msgstr "橡皮擦"

msgid "Stamps"
msgstr "圖章"

msgid "Shapes"
msgstr "形狀"

msgid "Letters"
msgstr "字母"

msgid "Magic"
msgstr "魔術"

