msgid "017"
msgstr "017"

# الأرقام
#. digits
msgid "O0"
msgstr "O0"

# فرق بين الأحرف الشبيهة بالدائرة
#. distinct circle-like characters
msgid "1Il|"
msgstr "1Il|"

