msgid "Great!"
msgstr "すごい！"

msgid "Cool!"
msgstr "うまいね！"

msgid "Keep it up!"
msgstr "そのちょうし！"

msgid "Good job!"
msgstr "じょうでき！"

msgid "Fill"
msgstr "ぬる"

msgid "Grass"
msgstr "くさ"

