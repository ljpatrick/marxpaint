#: magic.h:48
msgid "Rainbow"
msgstr "Regnboge"

#: magic.h:49
msgid "Sparkles"
msgstr "Gneistar"

#: magic.h:51
msgid "Mirror"
msgstr "Spegel"

#: magic.h:52
msgid "Flip"
msgstr "Snu"

#: magic.h:54
msgid "Blur"
msgstr "Uskarp"

#: magic.h:55
msgid "Blocks"
msgstr "Blokk"

#: magic.h:57
msgid "Negative"
msgstr "Negativ"

#: magic.h:58
msgid "Fade"
msgstr "Bleikna"

#: magic.h:60
msgid "Chalk"
msgstr "Krit"

#: magic.h:61
msgid "Drip"
msgstr "Drypping"

